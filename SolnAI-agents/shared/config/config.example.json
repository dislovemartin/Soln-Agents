{
    "api_port": 8080,
    "api_host": "0.0.0.0",
    "api_debug": false,
    "supabase_table": "messages",
    "llm_provider": "openai",
    "openai_model": "gpt-4o",
    "anthropic_model": "claude-3-sonnet-20240229",
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "file": "agent.log"
    },
    "cors": {
        "allow_origins": ["*"],
        "allow_methods": ["*"],
        "allow_headers": ["*"]
    },
    "rate_limiting": {
        "enabled": true,
        "requests_per_minute": 60
    }
}
